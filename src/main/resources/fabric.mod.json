{
  "schemaVersion": 1,
  "id": "ccpacks",
  "version": "${version}",
  "name": "Custom Content Packs",
  "description": "Data Driven blocks, items, tools and weapons, in minecraft!",
  "authors": [
    "ThatRobin",
    "PixelNinja"
  ],
  "license": "CC0-1.0",
  "icon": "assets/ccpacks/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.ThatRobin.ccpacks.CCPacksMain"
    ],
    "cardinal-components-entity": [
      "io.github.ThatRobin.ccpacks.CCPacksMain",
      "io.github.ThatRobin.ccpacks.Component.ModComponents"
    ],
    "cardinal-components-block": [
      "io.github.ThatRobin.ccpacks.Component.ModComponents"
    ],
    "client": [
      "io.github.ThatRobin.ccpacks.CCPacksClient"
    ]
  },
  "mixins": [
    "ccpacks.mixins.json"
  ],
  "custom": {
    "cardinal-components": [
      "ccpacks:saved_items",
      "ccpacks:choice",
      "ccpacks:mechanics"
    ],
    "modmenu": {
      "badges": [
        "library"
      ]
    }
  },
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.17.x"
  }
}
